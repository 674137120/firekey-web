.container {
  padding: 20rpx;
}

.search-box {
  padding: 20rpx;
  background: #fff;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
}

.search-box input {
  height: 80rpx;
  padding: 0 20rpx;
  background: #f5f5f5;
  border-radius: 8rpx;
}

.result-item {
  background: #fff;
  padding: 20rpx;
  margin-bottom: 10rpx;
  border-radius: 8rpx;
}

.name {
  font-size: 32rpx;
  color: #333;
  margin-bottom: 10rpx;
}

.address {
  font-size: 26rpx;
  color: #666;
} 